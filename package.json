{
  "name": "celtyui",
  "version": "1.0.2",
  "description": "headless源码组件库",
  "bin": "./es/index.mjs",
  "types": "./es/types/src/index.d.ts",
  "files": [
    "es"
  ],
  "scripts": {
    "clean": "rimraf es",
    "build-bin": "rollup --config build/rollup.config.ts --configPlugin typescript",
    "build-ui": "pnpm -r --filter \"celtyui-*\" build",
    "build-code": "run-p build-bin build-ui",
    "build": "run-s clean build-code copy",
    "copy": "esno build/copy.ts",
    "dev-vue": "pnpm --filter \"celtyui-vue\" dev",
    "build-vue": "pnpm --filter \"celtyui-vue\" build",
    "dev-react": "pnpm --filter \"celtyui-react\" dev",
    "build-react": "pnpm --filter \"celtyui-react\" build",
    "lint": "pnpm lint-src && pnpm lint-packages",
    "lint-src": "eslint --cache src/**/*.ts",
    "lint-packages": "pnpm -r --filter \"celtyui-react\" lint",
    "csadd": "changeset",
    "csver": "pnpm changeset version",
    "release-only": "changeset publish --registry=https://registry.npmjs.com/",
    "release": "run-s build release-only"
  },
  "keywords": [
    "celty",
    "celtyui",
    "ui",
    "headless",
    "vue3",
    "react",
    "ts",
    "scss",
    "source",
    "components",
    "源码",
    "组件库"
  ],
  "author": "zhouyinkui",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YamadaAoi/celtyui"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/koa": "^2.15.0",
    "@types/koa-static": "^4.0.4",
    "@types/madge": "^5.0.3",
    "@types/node": "^22.10.2",
    "@types/postcss-pxtorem": "^6.1.0",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "commander": "^12.1.0",
    "echarts": "^5.5.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "esno": "^4.8.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.14.0",
    "highlight.js": "^11.11.1",
    "kleur": "^4.1.5",
    "koa": "^2.15.3",
    "koa-static": "^5.0.0",
    "lint-staged": "^15.2.11",
    "madge": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.0",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^10.1.2",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.4.2",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass-embedded": "^1.83.0",
    "simple-git-hooks": "^2.11.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-checker": "^0.8.0",
    "zod": "^3.24.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --allow-empty --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{ts,tsx,vue}": [
      "eslint --cache --fix"
    ]
  }
}
