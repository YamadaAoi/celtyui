{
  "name": "vtscadd",
  "version": "1.0.4",
  "description": "vue3+ts+scss源码组件库",
  "bin": "./es/index.mjs",
  "types": "./es/types/src/index.d.ts",
  "files": [
    "es"
  ],
  "scripts": {
    "dev": "",
    "build": "pnpm depend && pnpm build-bin",
    "build-bin": "rollup --config build/rollup.config.ts --configPlugin typescript",
    "depend": "pnpm clean && esno build/depend.ts",
    "clean": "rimraf es",
    "lint": "pnpm lint-src && pnpm lint-build",
    "lint-src": "eslint --cache src/**/*.{ts,tsx,vue}",
    "lint-build": "eslint --cache build/**/*.{ts,tsx,vue}",
    "csadd": "changeset",
    "csver": "pnpm changeset version",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/",
    "release": "pnpm build && pnpm release:only"
  },
  "keywords": [
    "moyu",
    "vue3",
    "ts",
    "scss",
    "node",
    "源码组件库",
    "组件库"
  ],
  "author": "zhouyinkui",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YamadaAoi/vtscadd"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "kleur": "^4.1.5",
    "prompts": "^2.4.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@mo-yu/core": "^1.0.16",
    "@mo-yu/vue": "^1.0.21",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/madge": "^5.0.3",
    "@types/node": "^22.10.2",
    "@types/postcss-pxtorem": "^6.1.0",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "echarts": "^5.5.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "esno": "^4.8.0",
    "globals": "^15.14.0",
    "lint-staged": "^15.2.11",
    "madge": "^8.0.0",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^10.1.2",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "sass-embedded": "^1.83.0",
    "simple-git-hooks": "^2.11.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vue": "^3.5.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --allow-empty --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{ts,tsx,vue}": [
      "eslint --cache --fix"
    ]
  }
}
